{
  "version": 2,
  "builds": [
    {
      "src": "./",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ",/index.html",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://tunestellar.vercel.app"
      }
    }
  ]
}
